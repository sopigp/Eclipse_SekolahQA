package Sekolah_QA.com;

public class cow_conversation extends ikan {
	
	public void conversation2()
	{
		System.out.println("===================Cow Conversation==================");
	}

	@Override
	public void move() {
		System.out.println("The fish says : I move with my four legs ");
	}

	@Override
	public void live_in() {
		System.out.println("The fish says : I live in fresh water ");
	}

	@Override
	public void breed() {
		System.out.println("The fish says : I am a mammal, so I can give birth to reproduce offspring");
	}
}
